{
  "name": "cartistry-ios",
  "version": "1.0.0",
  "description": "Cartistry - Map Poster Generator iOS App",
  "main": "index.js",
  "scripts": {
    "build:web": "cd ../frontend && npm run export:ios && cp -r out ../ios-app/www",
    "sync": "npx cap sync ios",
    "open": "npx cap open ios",
    "build": "npm run build:web && npm run sync",
    "dev": "npm run build && npm run open",
    "init": "npx cap add ios",
    "clean": "rm -rf www ios/App/Pods"
  },
  "dependencies": {
    "@capacitor/app": "^6.0.0",
    "@capacitor/core": "^6.0.0",
    "@capacitor/haptics": "^6.0.0",
    "@capacitor/ios": "^6.0.0",
    "@capacitor/keyboard": "^6.0.0",
    "@capacitor/share": "^6.0.0",
    "@capacitor/status-bar": "^6.0.0",
    "@capacitor/splash-screen": "^6.0.0",
    "@capacitor/filesystem": "^6.0.0"
  },
  "devDependencies": {
    "@capacitor/cli": "^6.0.0"
  },
  "keywords": ["capacitor", "ios", "map", "poster", "cartistry"],
  "author": "Cartistry",
  "license": "MIT"
}
